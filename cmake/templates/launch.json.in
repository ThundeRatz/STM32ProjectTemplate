{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "cortex-debug",
            "request": "launch",
            "servertype": "stutil",
            "cwd": "${CMAKE_CURRENT_SOURCE_DIR}",
            "gdbPath": "arm-none-eabi-gdb",
            "executable": "build/${DEBUG_FILE_NAME}.elf",
            "name": "Cortex Debug (ST-Util)",
            "device": "${DEVICE}",
            "v1": false
        },
        {
            "type": "cortex-debug",
            "request": "launch",
            "servertype": "jlink",
            "cwd": "${CMAKE_CURRENT_SOURCE_DIR}",
            "gdbPath": "arm-none-eabi-gdb",
            "executable": "build/${DEBUG_FILE_NAME}.elf",
            "name": "Cortex Debug (J-Link)",
            "device": "${DEVICE}",
            "interface": "swd",
        }
    ]
}
