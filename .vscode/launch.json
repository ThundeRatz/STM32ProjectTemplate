    "version": "0.2.0",
    "configurations": [
        {
            "type": "gnu-debugger",
            "request": "launch",
            "name": "GNU debugger",
            "program": "${workspaceFolder}/build/main.elf",
            "toolchain": "/usr/bin",
            "client": "arm-none-eabi-gdb",
            "clientArgs": [],
            "server": "JLinkGDBServer",
            "serverArgs": [
                "-device", "STM32F303RE",
                "-if", "SWD"
            ],
            "serverPort": 2331
        }
    ]
}
